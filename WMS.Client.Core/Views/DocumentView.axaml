<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm ="clr-namespace:WMS.Client.Core.ViewModels"
			 mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="WMS.Client.Core.Views.DocumentView"
			 x:DataType="vm:DocumentViewModel">
	<StackPanel>
		<Label Content="{Binding Order.Name}"/>
		<Label Content="{Binding Order.Id}"/>
		<Label Content="{Binding Order.Number}"/>
		<Label Content="{Binding Order.DateTime}"/>
	</StackPanel>
</UserControl>
